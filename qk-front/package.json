{
  "name": "qualkey-front-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint:code": "next lint",
    "lint:styles": "npm run lint:common-styles && npm run lint:component-styles && npm run lint:layout-styles",
    "lint:common-styles": "stylelint --fix src/assets/styles/* --custom-syntax postcss-scss",
    "lint:component-styles": "stylelint --fix src/components/**/*.scss --custom-syntax postcss-scss",
    "lint:layout-styles": "stylelint --fix src/layouts/**/*.scss --custom-syntax postcss-scss",
    "prepare": "if [ \"$NODE_ENV\" = \"production\" ]; then husky install; fi"
  },
  "dependencies": {
    "@types/papaparse": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "axios": "^0.27.2",
    "classnames": "^2.3.1",
    "cookies-next": "^2.1.1",
    "moment": "^2.29.4",
    "next": "12.2.3",
    "nextjs-progressbar": "^0.0.14",
    "papaparse": "^5.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-pin-input": "^1.3.0",
    "react-qr-code": "^2.0.7",
    "recoil": "^0.7.5",
    "recoil-persist": "^4.2.0"
  },
  "devDependencies": {
    "@types/node": "^18.6.3",
    "@types/react": "18.0.15",
    "eslint": "8.21.0",
    "eslint-config-next": "12.2.3",
    "husky": "^8.0.0",
    "postcss-scss": "^4.0.4",
    "sass": "^1.54.1",
    "stylelint": "^14.9.1",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "typescript": "4.7.4"
  }
}
